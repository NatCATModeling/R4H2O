## Session 1 assignments

## Tidyverse
library(tidyverse)

turbidity <- read_csv("session2/turbidity_laanecoorie.csv")

ggplot(turbidity, aes(Result, fill = Zone)) +
    geom_histogram(binwidth = 0.1) +
    facet_wrap(~Zone) +
    labs(title = "Laanecoorie System",
         subtitle = "2018")
ggsave("manuscript/resources/session1/turbidity-distribution.png")
